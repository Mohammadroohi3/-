#include <stdio.h>

int main() {
    // اعداد کد ASCII برای هر حرف از "Mohammadroohi"
    int ascii_values[] = {77, 111, 104, 97, 109, 109, 97, 100, 114, 111, 111, 104, 105};
    
    printf("کد اسکی:\n");
    
    // چاپ هر کاراکتر با استفاده از اعداد کد ASCII
    for (int i = 0; i < sizeof(ascii_values) / sizeof(ascii_values[0]); ++i) {
        printf("%c ", ascii_values[i]);
    }
    
    printf("\n");
    return 0;
}
